            <div class="sb-login-form">
                <p>Login</p>
                <div class="sb-login-form-block sb-lg-cplt" data-validate="Ingresa tu email">
                    <input class="sb-lg-input" placeholder="" name="email" id="txtEmail" autocomplete="off" />
                    <label class="sb-lg-label" for="txtEmail">Email</label>
                </div>
                <div class="sb-login-form-block sb-lg-cplt" data-validate="Ingresa tu contraseña">
                    <input class="sb-lg-input" placeholder="" type="password" name="password" id="txtPassword" autocomplete="off" />
                    <label class="sb-lg-label" for="txtPassword">Contraseña</label>
                </div>
                <div class="sb-login-form-block sb-lg-btn">
                    <button type="button" id="btn_ing">
                        Ingresar
                    </button>
                </div>
                <div class="sb-login-form-block sb-lg-rec">
                    <a onclick="redireccionar('RecuperarContraseña', 2)" href="javascript:void(0)">
                        <p>¿Has olvidado tu contraseña?</p>
                    </a>
                </div>
            </div>
